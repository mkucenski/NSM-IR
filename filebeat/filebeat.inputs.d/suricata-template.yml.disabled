          - from: "init_spi"
            to: "key_init_spi"
          - from: "resp_spi"
            to: "key_response_spi"
          - from: "role"
            to: "key_role"
          - from: "errors"
            to: "key_errors"
          - from: "payload"
            to: "key_payload"
          - from: "notify"
            to: "key_notify"

    # Module: netflow
    - add_fields:
        when:
          has_fields: ['netflow']
