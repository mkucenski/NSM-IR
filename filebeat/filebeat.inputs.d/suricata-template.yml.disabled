- type: log
  enabled: false
  fields:
    log_type: suricata
    incident_number: ""
    incident_title: ""
    incident_dataset: ""
  fields_under_root: true
  json.keys_under_root: true

  paths:
    - /data/incidents/[incident_number]/[incident_dataset]/suricata/eve.json

  processors:
    - timestamp:
        field: timestamp
        layouts:
          - '2006-01-02T15:04:05.999999-0700'

    - drop_fields:
        fields: timestamp
