    - rename:
        when:
          has_fields: ['smb']
        ignore_missing: true
        fail_on_error: false
        fields:
          - from: "id"
            to: "smb_id"
          - from: "dialect"
            to: "application_protocol_dialect"
          - from: "command"
            to: "command"
          - from: "session_id"
            to: "uid"
          - from: "tree_id"
            to: "smb_tree_id"
          - from: "client_dialects"
            to: "application_protocol_client_dialects"
          - from: "filename"
            to: "file_name"
          - from: "disposition"
            to: "command"
          - from: "access"
            to: "file_open_state"
          - from: "created"
            to: "file_creation_time"
          - from: "accessed"
            to: "file_accessed_time"
          - from: "modified"
            to: "file_modified_time"
          - from: "changed"
            to: "file_changed_time"
          - from: "size"
            to: "file_size"
          - from: "fuid"
            to: "file_uid"
          - from: "share"
            to: "share_name"
          - from: "request.native_os"
            to: "smb_native_os_request"
          - from: "request.native_lm"
            to: "smb_native_lm_request"
          - from: "response.native_os"
            to: "smb_native_os_response"
          - from: "response.native_lm"
            to: "smb_native_lm_response"

    # Module: smtp
    - add_fields:
        when:
          has_fields: ['smtp']
        fields:
          event_type: smtp
    - rename:
        when:
          has_fields: ['smtp']
        ignore_missing: true
        fail_on_error: false
